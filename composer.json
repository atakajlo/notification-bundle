{
  "name": "atakajlo/notification-bundle",
  "type": "symfony-bundle",
  "description": "simple notification bundle for symfony",
  "minimum-stability": "dev",
  "license": "proprietary",
  "authors": [
    {
      "name": "Dmytro Fedorenko",
      "email": "atakajlo@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Atakajlo\\NotificationBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Atakajlo\\NotificationBundle\\Tests\\": "Tests/"
    }
  },
  "require": {
    "php": "^7.4",
    "atakajlo/notifications": "@dev",
    "centrifugal/phpcent": "^3.1",
    "nyholm/dsn": "^2.0.0-beta1",
    "symfony/framework-bundle": "^4.3.8|^5.0",
    "symfony/mailer": "^4.3.8|^5.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9"
  },
  "prefer-stable": true
}
